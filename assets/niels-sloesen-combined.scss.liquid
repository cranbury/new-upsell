  .select-selected:after {
    border-style: solid;
    border-width: 2px 2px 0 0;
    content: '';
    display: inline-block;
    height: 0.35rem;
    position: absolute;
    vertical-align: top;
    width: 0.35rem;
    transform: rotate(135deg);
    right: 12px;
    transition: transform 0.2s;
  }
  
